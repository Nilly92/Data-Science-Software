# Graphics.kv file of the code
  
# Creating Graphics for Start Page 
  
# Rectangle 
  
<Frame>
    
    # creating canvas
    canvas:
        
        Color:
            rgba: rgba ('#003A51')
  
        # size and position of Canvas
        Line: 
            width: 5
            rectangle:self.x+20, self.y-130, self.width-40, self.height+100
            
                    
            
                  
<StartButton>:
  
        
         
    Button:
        background_normal: ''
        background_color: rgba ('#003A51')
        text: "Let's Go"
        size: 150,150
        pos: 600,80
        bold:True

<Logo>:

    Image:
        source: 'DataVisu.jpeg'
        
        size: 500, 500
        pos: 0,-100
        
<Box>:
   
    Image:
        source: 'PieChart.jpg'
        size: 250, 300
        pos:25,300
    
    Image:
        source: 'Line.jpg'
        size: 250, 300
        pos:275,300
    
    Image:
        source: 'BarChart.jpg'
        size: 250, 300
        pos:525,300    
      
  
                    
   
   